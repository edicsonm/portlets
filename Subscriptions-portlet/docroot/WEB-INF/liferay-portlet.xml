<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.2.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_2_0.dtd">

<liferay-portlet-app>
	<portlet>
		<portlet-name>Subscriptions</portlet-name>
		<icon>/icon.png</icon>
		<friendly-url-mapper-class>com.liferay.portal.kernel.portlet.DefaultFriendlyURLMapper</friendly-url-mapper-class>
		<friendly-url-mapping>Suscription</friendly-url-mapping>
		<friendly-url-routes>au/com/billingbuddy/porlet/urlRoutes/friendly-url-routes.xml</friendly-url-routes>
		<show-portlet-access-denied>false</show-portlet-access-denied>
		<action-url-redirect>true</action-url-redirect>
		<header-portlet-css>/css/main.css</header-portlet-css>
        <header-portlet-css>/css/jquery.json-viewer.css</header-portlet-css>
		<footer-portlet-javascript>/js/main.js</footer-portlet-javascript>
        <footer-portlet-javascript>/js/jquery-2.1.3.min.js</footer-portlet-javascript>
        <footer-portlet-javascript>/js/jquery.json-viewer.js</footer-portlet-javascript>
		<css-class-wrapper>Subscriptions-portlet</css-class-wrapper>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>user</role-name>
		<role-link>User</role-link>
	</role-mapper>
</liferay-portlet-app>